<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <stroke
                android:width="1dp"
                android:color="#ffd9d9d9" />
            <gradient
                android:angle="270"
                android:centerColor="#ffd4d4d4"
                android:centerY="0.45"
                android:endColor="#ffe4e4e4"
                android:startColor="#ffc6c6c6" />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <stroke
                    android:width="1dp"
                    android:color="#ff2aa3d3" />
                <gradient
                    android:angle="270"
                    android:centerColor="#ff21b2df"
                    android:centerY="0.45"
                    android:endColor="#ff0d8ec2"
                    android:startColor="#ff33d2fa" />
            </shape>
        </clip>
    </item>

</layer-list>